    neuron_4 -> neuron_7 [label="0.20", fontcolor="#333333"];
    neuron_4 -> neuron_9 [label="0.31", fontcolor="#333333"];
    neuron_5 -> neuron_6 [label="1.90", fontcolor="#333333"];
    neuron_5 -> neuron_7 [label="1.15", fontcolor="#333333"];
